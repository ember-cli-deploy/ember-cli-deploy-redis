{
  "name": "ember-cli-deploy-redis",
  "version": "1.0.3",
  "description": "Ember CLI Deploy plugin to deploy file(s) into redis.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "release": "release-it",
    "test": "node tests/runner.js && ./node_modules/.bin/eslint index.js lib/* tests/**/*.js"
  },
  "repository": "https://github.com/ember-cli-deploy/ember-cli-deploy-redis",
  "engines": {
    "node": ">= 10"
  },
  "author": "Aaron Chambers and the ember-cli-deploy team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.0.0",
    "core-object": "^3.1.5",
    "ember-cli-deploy-plugin": "^0.2.9",
    "ioredis": "^3.2.2",
    "rsvp": "^4.8.5"
  },
  "devDependencies": {
    "auto-changelog": "^2.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "ember-cli": "^3.18.0",
    "eslint": "^7.0.0",
    "ioredis-mock": "^3.14.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "release-it": "*",
    "sinon": "^9.0.2"
  },
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "git": {
      "changelog": "npx auto-changelog --stdout --commit-limit false --unreleased --template https://raw.githubusercontent.com/release-it/release-it/master/templates/changelog-compact.hbs"
    },
    "hooks": {
      "after:bump": "npx auto-changelog -p"
    },
    "github": {
      "release": true
    }
  }
}
