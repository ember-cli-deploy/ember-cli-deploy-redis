{
  "name": "ember-cli-deploy-redis",
  "version": "3.1.0",
  "description": "Ember CLI Deploy plugin to deploy file(s) into redis.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "release": "release-it",
    "test": "node tests/runner.js && ./node_modules/.bin/eslint index.js lib/* tests/**/*.js"
  },
  "repository": "https://github.com/ember-cli-deploy/ember-cli-deploy-redis",
  "engines": {
    "node": ">= 12"
  },
  "author": "Aaron Chambers and the ember-cli-deploy team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.1",
    "core-object": "^3.1.5",
    "ember-cli-deploy-plugin": "^0.2.9",
    "ioredis": "^4.27.5",
    "rsvp": "^4.8.5"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "ember-cli": "^3.26.1",
    "eslint": "^7.28.0",
    "glob": "^7.1.7",
    "ioredis-mock": "^5.6.0",
    "lerna-changelog": "^1.0.1",
    "mocha": "^8.4.0",
    "release-it": "14.8.0",
    "release-it-lerna-changelog": "^3.1.0",
    "sinon": "^11.1.1"
  },
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "v${version}",
      "pushArgs": "--follow-tags --no-verify"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true
    },
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    }
  },
  "volta": {
    "node": "14.18.1",
    "yarn": "1.22.17"
  }
}
